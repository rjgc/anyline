<?xml version='1.0' encoding='utf-8'?>
<widget id="io.anyline.examples.cordova" version="3.24.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="AnylineSDK">
        <param name="android-package" value="io.anyline.cordova.AnylinePlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Anyline Cordova Example</name>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="deployment-target" value="8.0" />
    <preference name="Orientation" value="portrait" />
</widget>
